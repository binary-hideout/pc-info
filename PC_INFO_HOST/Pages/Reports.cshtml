@page
@model PC_INFO_HOST.Pages.ReportsModel
@{
    ViewData["Title"] = "Reports";
}
<h1>Reportes</h1>
<table class="table">
    <thead>
        <tr>
            <th>Camino del archivo</th>
            <th>Descargar</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Path)
        {
            <tr>
                <td>@item</td>
                <td><a href="@Url.Content(@item)" target="_blank">Ver</a></td>
            </tr>
        }
    </tbody>
</table>

